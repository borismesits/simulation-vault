   "source": [
    "import numpy as np\n",
    "\n",
    "kB = 1.38e-23\n",
    "h = 6.62e-34\n",
    "\n",
    "f = 5e9\n",
    "T = 0.4\n",
    "\n",
    "nbar = 1 / ( np.exp(h*f / (kB*T)) - 1 )\n",
    "\n",
    "print(nbar)\n",
    "\n",
    "kappa = 1e6*2*np.pi\n",
    "chi = 12e3*2*np.pi\n",
    "\n",
    "tphi = kappa / (nbar * (nbar+1) * chi**2)\n",
    "\n",
    "print(tphi)"
   ]
